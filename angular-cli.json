{
  "project": {
    "version": "1.0.0-beta.26",
    "name": "nwi-web-fcc-admin"
  },
  "apps": [
    {
      "root": "src/main/web",
      "outDir": "target/classes/templates",
      "assets": [
        "assets",
        "font",
        "favicon.ico",
        "welcome.html",
        "login.html",
        "logout.html"
      ],
      "index": "index.html",
      "main": "main.ts",
      "test": "test.ts",
      "tsconfig": "../../../tsconfig.json",
      "prefix": "app",
      "mobile": false,
      "styles": [
        "../../../node_modules/bootstrap/dist/css/bootstrap.css",
        "font/typicons.css"
      ],
      "scripts": [
        "../../../node_modules/jquery/dist/jquery.js",
        "../../../node_modules/bootstrap/dist/js/bootstrap.js"

      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "prefixInterfaces": false,
    "inline": {
      "style": false,
      "template": false
    },
    "spec": {
      "class": false,
      "component": true,
      "directive": true,
      "module": false,
      "pipe": true,
      "service": true
    }
  }
}
